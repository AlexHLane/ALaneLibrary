@model ALaneLibrary.ViewModels.MemberViewModel
@{
    ViewBag.Title = "Index";
}
<h3>BOOK INDEX</h3>
<h2>List of Books and list of Customers together</h2>

<h4>List of Books</h4>
<ul>
        @foreach (var book in Model.BookCollection)
        {
            <li>
                @book.Title;
            </li>

        }
</ul>


<h4>List of Customers</h4>

    <ul>
        @foreach (var customer in Model.MemberList)
        {
            <li>
                @customer.MemberName;
            </li>

        }
    </ul> 